{
  "name": "my-locales",
  "version": "0.2.0",
  "private": true,
  "types": "dist/*.d.mts",
  "typesVersions": {
    "*": {
      "*": ["dist/index.d.mts"],
      "constants": ["dist/constants.d.mts"]
    }
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./constants": "./dist/constants.mjs",
    "./mdx/*": "./mdx/*"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "SWC_NODE_PROJECT=./tsconfig.json node --no-warnings --loader @swc-node/register/esm --test-reporter spec --test tests/*.test.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "my-constants": "workspace:*"
  },
  "devDependencies": {
    "@swc-node/register": "1.10.9",
    "tsconfig": "workspace:*",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  }
}
