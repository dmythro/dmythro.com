{
  "name": "dmythro-com",
  "packageManager": "bun@1.2.18",
  "private": false,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:clean": "turbo build --cache=local:r,remote:r",
    "build:force": "turbo build --force",
    "bump": "bun outdated -F '*'",
    "check-types": "turbo check-types",
    "ci": "biome ci .",
    "clean": "turbo clean",
    "cv": "turbo cv --force",
    "dev": "turbo dev",
    "check": "biome check .",
    "check-apply": "biome check --write --unsafe .",
    "lint": "biome lint .",
    "start": "turbo start"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "turbo": "2.5.4"
  },
  "dependencies": {
    "@types/bun": "1.2.18",
    "tailwindcss": "4.1.11"
  }
}
